    # Caso 6: var_true yunta var_false -> Nolsa
    culito case6 : tas_claro = var_true yunta var_false;

    # Caso 7: (Sisa yunta Sisa) yunta (Nolsa yunta Sisa) -> Sisa yunta Nolsa -> Nolsa
    culito case7 : tas_claro = (Sisa yunta Sisa) yunta (Nolsa yunta Sisa);
