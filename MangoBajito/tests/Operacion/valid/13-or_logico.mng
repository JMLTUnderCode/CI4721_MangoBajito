    # Caso 6: var_false o_sea var_true -> Sisa
    resultado = var_false o_sea var_true;

    # Caso 7: (Sisa o_sea Nolsa) o_sea (Nolsa o_sea Nolsa) -> Sisa o_sea Nolsa -> Sisa
    resultado = (Sisa o_sea Nolsa) o_sea (Nolsa o_sea Nolsa);
};