se_prende(){
    culito A : mango[2][2] = [[1, 2], [3, 4]];
    culito B : mango[2][2] = [[5, 6], [7, 8]];
    
    culito C: mango[2][2];

    repite_burda i entre 0 hasta 2 {
        repite_burda j entre 0 hasta 2 {
            C[i][j] = 0;
        };
    };
    
    repite_burda i entre 0 hasta 2 {
        repite_burda j entre 0 hasta 2 {
            repite_burda k entre 0 hasta 2 {
                C[i][j] += A[i][k] * B[k][j];
            };
        };
    };

    repite_burda i entre 0 hasta 2 {
        repite_burda j entre 0 hasta 2 {
            rescata(3);
        };
    };
};