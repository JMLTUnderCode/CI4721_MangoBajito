# Test 05: Operacion de decremento.
se_prende(){
	# Debe tener un valor para poder aplicar decremento.
	culito decl0: mango = -8;
	decl0--;

	culito decl1: manguita = 2.1;
	decl1--;

	culito decl2: manguangua = 1.21e+10;
	decl2--;

	# A excepcion de las funciones.
    echar_cuento bar0(x: mango) lanza un_coño {
		x--;
	};

	echar_cuento bar1(x: manguita) lanza un_coño {
		x--;
	};

	echar_cuento bar2(x: manguangua) lanza un_coño {
		x--;
	};
};