# Test 03: Operacion de division decimal.
culito div_entera_mango: mango;
culito div_entera_manguita: manguita;
culito div_entera_manguangua: manguangua;
se_prende(){
	culito n0: mango = -8;
	n0 = n0 / 11;
	n0 = n0 / 8 / 4;
	n0 = (n0 / 1) / (n0 / 2);
	div_entera_mango = n0 / 1;

	culito n1: manguita = -8.9;
	n1 = n1 / 11.2;
	n1 = n1 / 8.7 / 4.12;
	n1 = (n1 / 1.23) / (n1 / 2.22);
	div_entera_manguita = n1 / 1.21;

	culito n2: manguangua = 8.2e+10;
	n2 = n2 / 11.2e+10;
	n2 = n2 / 8.2e+10 / 4.2e+10;
	n2 = (n2 / 1.2e+10) / (n2 / 2.2e+10);
	div_entera_manguangua = n2 / 1.2e+10;
};