# Test 01: Operacion de resta.
culito resta_mango: mango;
culito resta_manguita: manguita;
culito resta_manguangua: manguangua;
se_prende(){
	culito n0: mango = -8;
	n0 = n0 - 11;
	n0 = n0 - 8 - 4;
	n0 = (n0 - 1) - (n0 - 2);
	resta_mango = n0 - 1;

	culito n1: manguita = -8.9;
	n1 = n1 - 11.2;
	n1 = n1 - 8.7 - 4.12;
	n1 = (n1 - 1.23) - (n1 - 2.22);
	resta_manguita = n1 - 1.21;

	culito n2: manguangua = 8.2e+10;
	n2 = n2 - 11.2e+10;
	n2 = n2 - 8.2e+10 - 4.2e+10;
	n2 = (n2 - 1.2e+10) - (n2 - 2.2e+10);
	resta_manguangua = n2 - 1.2e+10;
};