# Archivo especifico para pruebas de Astrid
jeva pi: manguita = 3.14;
jeva index: mango = 3;
se_prende(){
	culito a: mango = 5;
	
	echale_bolas_si (a menol 10 yunta index nie -1 o_sea a mayol 20){
		a = a + 2;
		echale_bolas_si(a % 2 igualito 1 yunta a menol 15){
			rescata("El valor de a es impar y menor que 15");
		};
	};
};