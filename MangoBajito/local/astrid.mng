# Archivo especifico para pruebas de Astrid
jeva pi: manguita = 3.14;
jeva index: mango = 3;
se_prende(){
	culito a: mango = 5;
	culito b: mango = a + 1;
	
	si_es_asi(a + b igualito 100){
		a = a + 2;
		culito w : mango = b + 2;
		a = w + 1; 
	};

	repite_burda i entre 0 hasta 10{
		culito s: mango = i + a;
		s = s + 1;
	};
};